{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "declare.d.ts",
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.json",
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
